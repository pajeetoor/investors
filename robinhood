public class Main {
    public static void main(String[] args) {
        // First block of dashes
        for (int i = 0; i < 8; i++) {
            System.out.println("------------------------------------------------------------------------------------------");
        }

        // Main part of the pattern with "@" symbols, "%" symbols, and the middle row with special characters
        System.out.println("---------------------------------------------------+*##%%%%%%%#*+-------------------------");
        System.out.println("------------------------------------------------=#@@@@@@@@@@@@@@@%=-----------------------");
        System.out.println("----------------------------------------------+%@@@@@@@@@@@@@@@@@@%-----------------------");
        System.out.println("--------------------------------------------+%@@@@@@@@@@@@@@@@@@@@@+----------------------");
        System.out.println("------------------------------------------+%@@@@@@@@@@@@@@@@@@@@@@@*----------------------");
        System.out.println("------------------------------------------++++++++++++++#@@@@@@@@@@*----------------------");
        System.out.println("---------------------------------------++++++++++++=-----@@@@@@@@@@=----------------------");
        System.out.println("-------------------------------------*%@@@@@@@@@@%*------@@@@@@@@@*-----------------------");
        System.out.println("-----------------------------------=%@@@@@@@@@@@*---=%=--@@@@@@@%+------------------------");
        System.out.println("----------------------------------*@@@@@@@@@@@%=---#@@=--@@@@@@#--------------------------");
        System.out.println("--------------------------------+%@@@@@@@@@@%+---+%@@@=--@@@@%+---------------------------");
        System.out.println("------------------------------=#@@@@@@@@@@@#---=%@@@@@=--@@@#-----------------------------");
        System.out.println("-----------------------------+%@@@@@@@@@@@*---*@@@@@@@=--@%+------------------------------");
        System.out.println("----------------------------=@@@@@@@@@@@#=--=%@@@@@@@@=--#--------------------------------");
        System.out.println("----------------------------+@@@@@@@@@@*---+@@@@@@@@@@=-----------------------------------");
        System.out.println("----------------------------+@@@@@@@@%+---#@@@@@@@@@@@------------------------------------");
        System.out.println("----------------------------+@@@@@@@%=--=%@@@@@@@@@@@+------------------------------------");
        System.out.println("----------------------------+@@@@@@#---+%@@@@@@@@@@%=-------------------------------------");
        System.out.println("----------------------------+@@@@@*---*@@@@@@@@@@@#=--------------------------------------");
        System.out.println("----------------------------+@@@@*---#@@@@@@@@@@@#----------------------------------------");
        System.out.println("----------------------------%@@@*---#@@@@@@@@@@@*-----------------------------------------");
        System.out.println("---------------------------*@@@+---%@@@@@@%##*+-------------------------------------------");
        System.out.println("--------------------------=@@@*---%@%#*+=-------------------------------------------------");
        System.out.println("--------------------------%@@*---==-------------------------------------------------------");
        System.out.println("-------------------------*@@*-------------------------------------------------------------");
        System.out.println("------------------------=@@#--------------------------------------------------------------");
        System.out.println("------------------------%@%---------------------------------------------------------------");
        System.out.println("-----------------------+@%=---------------------------------------------------------------");
        System.out.println("-----------------------%%=----------------------------------------------------------------");

        // Last block of dashes
        for (int i = 0; i < 8; i++) {
            System.out.println("------------------------------------------------------------------------------------------");

          System.out.println("backed by robinhood");
        }
    }
}
